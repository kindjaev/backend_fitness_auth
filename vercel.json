{
    "version": 2,
    "builds": [
       { "src": "server.js", "use": "@vercel/node" }
    ],
    "rewrites": [
       { "src": "/(.*)", "dest": "/server.js" }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          { "key": "Access-Control-Allow-Origin", "value": "*" }
        ]
      }
  ]
 }